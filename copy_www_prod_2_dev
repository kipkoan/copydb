echo -e 'mysql -u root -p www_dev < mkdevdrupal.sql'
mysql -u root -p < mkdevdrupal.sql

echo -e '\nmysqldump -u root -p www_prod > output.www_prod.sql'
mysqldump -u root -p www_prod > output.www_prod.sql

echo -e '\nmysql -u root -p www_dev < output.www_prod.sql'
mysql -u root -p www_dev < output.www_prod.sql
